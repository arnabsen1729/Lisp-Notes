(print 3/7   )    ; ratio
(print 1.0   )    ; floating-point number one in default precision
(print 1.0e0 )    ; floating-point number
(print 1.0d0 )    ; the floating-point number one in "double" precision
(print 1.0e-4)    ; the floating-point
(print +42   )    ; the integer positive
(print -42   )    ; the integer negative
